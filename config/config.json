{
  "bot_token": "123456789:YOUR_TELEGRAM_BOT_TOKEN",
  "chat_id_default": 111111111,
  "poll_timeout": 25,
  "plugins_dir": "/opt/openwrt-telebot/plugins",
  "log_file": "/var/log/openwrt-telebot.log",
  "ui_api_token": "",
  "ui_base_url": "/telebot",
  "client_state_file": "/opt/openwrt-telebot/state/clients.json",
  "nft_table": "telebot",
  "nft_chain": "client_guard",
  "nft_block_set": "blocked_clients",
  "nft_allow_set": "approved_clients",
  "nft_internet_block_set": "internet_block_clients",
  "wan_interfaces": ["wan", "wan6"],
  "firewall_include_path": "/etc/nftables.d/telebot.nft",
  "firewall_include_section": "telebot_include",
  "enhanced_notifications": true,
  "notification_schedule": [],
  "version_endpoint": "https://api.github.com/repos/sfdcai/openwrt-telegram/releases/latest",
  "version_cache_ttl": 3600,
  "update_timeout": 600,
  "update_zip_url": "",
  "nft_binary": "nft",
  "nft_family": "inet",
  "dhcp_leases_path": "/tmp/dhcp.leases",
  "ip_neigh_command": "ip neigh show dev br-lan",
  "client_whitelist": []
}
